    "def get_embed_matrix(sentence):\n",
    "    \n",
    "    embeds = []\n",
    "    tokens = nltk.word_tokenize(sentence)\n",
    "    \n",
    "    for i in tokens:\n",
    "        \n",
    "        if i in vocab:\n",
    "            \n",
    "            n = vocab.index(i)\n",
    "        else :\n",
    "            \n",
    "            n = vocab.index('UNK')\n",
    "        embeds.append(n)\n",
    "    return np.array(embeds)"
   ]
