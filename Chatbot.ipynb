    "vocab = []\n",
    "\n",
    "for index,row in data.iterrows():\n",
    "    \n",
    "    tokens = nltk.word_tokenize(row['Query'])\n",
    "    for i in tokens :\n",
    "        \n",
    "        if i not in vocab:\n",
    "            \n",
    "            vocab.append(i)\n",
    "vocab.append('UNK') #Unknown token\n",
    "vocab.append('PAD') #Pad token"
